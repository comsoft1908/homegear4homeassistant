    "SYS_ADMIN",
    "SYS_RAWIO",
    "SYS_MODULE"
  ],
  "devices": ["/dev/spidev0.0"],
  "options": {
  },
  "map": ["config:rw", "share:rw", "ssl:rw"],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {  },
  "schema": {  }
}
