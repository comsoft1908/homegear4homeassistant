{
  "name": "Homegear4Homeassistant",
  "version": "1.0",
  "slug": "homegear4homeassistant",
  "description": "Homegear as a Hassio add-on",
  "arch": ["armhf"],
  "startup": "services",
  "boot": "auto",
  "kernel_modules": true,
  "uart": true,
  "gpio": true,
  "apparmor": false,
