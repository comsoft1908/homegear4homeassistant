{
  "name": "Homegear4Homeassistant",
  "version": "1.0",
  "slug": "homegear4homeassistant",
  "description": "Homegear as a Hassio add-on",
  "arch": ["armhf"],
  "startup": "before",
  "boot": "auto",
  "kernel_modules": true,
  "auto_uart": true,
  "gpio": true,
  "apparmor": false,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO",
    "SYS_MODULE"
  ],
  "devices": ["/dev/spidev0.0:/dev/spidev0.0:rwm", "/sys:/sys"],
  "options": {
    "device": null
  },
  "map": ["config:rw", "share:rw", "ssl:rw"],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {  },
  "schema": {  },
  "image": "homeassistant/addon-homegear"
}
